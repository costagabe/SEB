<div class="col s10 offset-s1">
    <h3 class="center-text">Cadastrar Aluno</h3>
    <div class="input-field col s8 offset-s2">
        <input id="nomeAluno" type="text" name="nomeAluno" class="validate">
        <label for="nomeAluno">Nome do Aluno</label>
    </div>
    <div class="input-field col s8 offset-s2">
        <input id="usuarioAluno" type="text" class="validate">
        <label for="usuarioAluno">Usuário Aluno</label>
    </div>
    <div class="input-field col s8 offset-s2">
        <input id="senhaAluno" type="password"  class="validate">
        <label for="senhaAluno">Senha</label>
    </div>
    <div class="col s2 offset-s8">
        <a class="waves-effect waves-teal btn  fill-width center-text" onclick="cadastrarAluno()">Cadastrar</a>
    </div>
</div>